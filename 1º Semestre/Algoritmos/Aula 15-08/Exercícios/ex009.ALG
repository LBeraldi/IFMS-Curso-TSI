Algoritmo "semnome"

Var
  a, b, c, d : caractere
Inicio
   Escreval("Vamos fazer seu cadastro, digite um login, e uma senha")
   leia(a, b)
   escreval("Agr vamos entrar no seu cadastro")
   escreval("digite o login e a senha")
   leia(c, d)
   se (c = a) e (c=d)  entao
   escreval("Agr vc tem acesso ao seu cadastro")
   senao
   escreval("Login ou senha inválidos")
   fimse
Fimalgoritmo


